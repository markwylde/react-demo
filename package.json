{
  "name": "react-demo",
  "private": true,
  "version": "0.0.1",
  "description": "A demo application to show react",
  "author": [
    "mark.wylde@verl.com"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:markwylde/react-demo.git"
  },
  "scripts": {
    "build": "npm run unit_tests && ./node_modules/.bin/gulp build",
    "lint": "./node_modules/.bin/gulp lint",
    "dev": "webpack-dev-server --hot --inline --config webpack_dev.config.js",
    "unit_tests": "./node_modules/.bin/gulp unit_tests",
    "unit_tests_travis": "./node_modules/.bin/gulp unit_tests && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "e2e_tests": "./node_modules/protractor/bin/webdriver-manager update && ./node_modules/protractor/bin/protractor protractor.conf.js",
    "test": "npm run lint && npm run unit_tests",
    "test_travis": "npm run lint && npm run unit_tests_travis"
  },
  "dependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "babel-plugin-rewire": "^0.1.22",
    "bootstrap": "^3.3.5",
    "chai": "^3.2.0",
    "coveralls": "^2.11.4",
    "css-loader": "^0.18.0",
    "css-reset": "0.0.1",
    "eslint": "^1.0.0",
    "eslint-plugin-react": "^3.3.2",
    "file-loader": "^0.8.4",
    "flux": "^2.0.3",
    "font-awesome": "^4.4.0",
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.0.0",
    "gulp-istanbul-plus": "^0.10.2",
    "gulp-mocha": "^2.1.3",
    "gulp-util": "^3.0.6",
    "gutil": "^1.6.4",
    "isparta": "^3.0.4",
    "jquery": "^2.1.4",
    "jsdom": "^6.4.0",
    "mocha": "^2.3.2",
    "mocha-sinon": "^1.1.4",
    "node-sass": "^3.2.0",
    "protractor": "^2.2.0",
    "react": "^0.13.3",
    "react-bootstrap": "^0.25.1",
    "react-hot-loader": "^1.2.8",
    "rimraf": "^2.4.3",
    "sass-loader": "^2.0.1",
    "sinon": "^1.16.1",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.11.0"
  }
}
